{
    "name": "softcodex/oidc-proxy",
    "description": "OIDC Proxy for M365 login",
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^7.2",
        "symfony/routing": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/config": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "league/oauth2-client": "^2.6",
        "symfony/dotenv": "^7.3",
        "symfony/http-client": "*",
        "symfony/cache": "^7.3",
        "symfony/mailer": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "OidcProxy\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
